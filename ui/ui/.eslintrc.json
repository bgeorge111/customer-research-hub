{
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "rules": {
    "jsx-a11y/anchor-has-content": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "useTabs": false
      }
    ]
  }
}
